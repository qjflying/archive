    },
    "dependencies": {
        "axios": "^0.19.0",
        "base-64": "^0.1.0",
        "core-js": "^3.3.2",
        "highlight.js": "^10.0.3",
        "jquery": "^3.5.1",
        "jstransformer-markdown-it": "^2.1.0",
        "lodash": "^4.17.15",
        "markdown-it": "^9.0.1",
        "marked": "^0.7.0",
        "utf8": "^2.1.2",
        "vue": "^2.6.10",
        "vue-highlightjs": "^1.3.3",
        "vue-router": "^3.1.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "cross-env": "^6.0.3",
        "echarts": "^4.5.0",
        "shelljs": "^0.8.4",
        "style-resources-loader": "^1.2.1",
        "stylus": "^0.54.7",
        "stylus-loader": "^3.0.2",
        "vue-cli-plugin-pug": "^1.0.7",
        "vue-cli-plugin-style-resources-loader": "^0.1.3",
        "vue-template-compiler": "^2.6.10",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-spritesmith": "^1.0.2"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "fecs": {
        "eslint": {
            "env": {
                "node": true
            },
            "rules": {
                "no-console": 0
            }
        },
        "csshint": {},
        "htmlcs": {},
        "jformatter": {},
        "esformatter": {},
        "csscomb": {}
    },
    "jscsConfig": {}
}
